	  <Compile Include="Compression.fsx" />
	  <Compile Include="Memory.fsx" />
	  <Compile Include="PB.fsx" />
	  <Compile Include="PB2.fsx" />
	  <Compile Include="CSL.fsx" />
	  <Compile Include="CSL2.fsx" />
	  <Compile Include="Task.fsx" />
